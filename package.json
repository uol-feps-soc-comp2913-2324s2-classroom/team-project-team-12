		"decimal.js": "^10.4.3",
		"express": "^4.18.3",
		"express-session": "^1.18.0",
		"js-cookie": "^3.0.5",
		"jsonwebtoken": "^9.0.2"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.js",
		"postdeploy": "npm run prisma:seed"
	}
