{
    "name": "journeys",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "start": "concurrently --kill-others \"npm:electron:dev\" \"npm:electron:start\"",
        "deploy": "npm i && vite build && vite preview --host",
        "electron:dev": "vite preview --port 3000",
        "electron:start": "electron src/electron.js",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write .",
        "prisma:seed": "prisma db seed --preview-feature"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/adapter-node": "^5.0.1",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@testing-library/svelte": "^4.1.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.7",
        "@types/eslint": "^8.56.0",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/google.maps": "^3.55.4",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/leaflet": "^1.9.8",
        "@types/leaflet-providers": "^1.2.4",
        "@types/multer": "^1.4.11",
        "@types/ramda": "^0.29.11",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "@vitest/ui": "^1.6.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.35.1",
        "flowbite": "^2.3.0",
        "flowbite-svelte": "^0.44.24",
        "flowbite-svelte-icons": "^1.4.5",
        "jsdom": "^24.0.0",
        "postcss": "^8.4.38",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.1.2",
        "prisma": "^5.13.0",
        "sass": "^1.72.0",
        "stripe": "^14.21.0",
        "sv-popup": "^0.5.3",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "svelte-stripe": "^1.1.4",
        "tailwindcss": "^3.4.3",
        "ts-node": "^10.9.2",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.3",
        "vitest": "^1.2.0"
    },
    "type": "module",
    "dependencies": {
        "@googlemaps/js-api-loader": "^1.16.6",
        "@prisma/client": "^5.10.2",
        "bad-words": "^3.0.4",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "concurrently": "^8.2.2",
        "cookie": "^0.6.0",
        "cookie-parser": "^1.4.6",
        "decimal.js": "^10.4.3",
        "electron": "^29.3.0",
        "express": "^4.19.2",
        "express-session": "^1.18.0",
        "gpxparser": "^3.0.8",
        "js-cookie": "^3.0.5",
        "jsonwebtoken": "^9.0.2",
        "leaflet": "^1.9.4",
        "leaflet-gesture-handling": "^1.2.2",
        "leaflet-providers": "^2.0.0",
        "leaflet-textpath": "^1.2.3",
        "multer": "^1.4.5-lts.1",
        "ramda": "^0.29.1",
        "svrollbar": "^0.12.0"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.js",
        "postdeploy": "npm run prisma:seed"
    }
}
